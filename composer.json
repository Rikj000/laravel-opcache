{
    "name": "rikj000/laravel-opcache",
    "description": "PHP OPcache Artisan commands for Laravel.",
    "keywords": [
        "rikj000",
        "opcache",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/rikj000/laravel-opcache",
    "license": "MIT",
    "authors": [
        {
            "name": "Rik Helsen",
            "email": "rik_helsen@hotmail.com",
            "homepage": "https://github.com/rikj000",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Rikj000\\Opcache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rikj000\\Opcache\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rikj000\\Opcache\\OpcacheServiceProvider"
            ]
        }
    }
}
